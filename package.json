{
  "name": "efayda-id-generator",
  "version": "1.0.0",
  "description": "Ethiopian National ID (eFayda) Card Generator Telegram Bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "mongodb:start": "cd mongodb-local && start-mongodb.bat",
    "mongodb:stop": "cd mongodb-local && stop-mongodb.bat",
    "mongodb:connect": "cd mongodb-local && connect-mongodb.bat"
  },
  "keywords": [
    "telegram",
    "bot",
    "efayda",
    "ethiopian",
    "national-id",
    "pdf",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@jvhaile/cbe-verifier": "^1.0.0",
    "canvas": "^3.2.0",
    "cheerio": "^1.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsbarcode": "^3.11.6",
    "mongoose": "^8.0.0",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.14.0",
    "qrcode": "^1.5.3",
    "sharp": "^0.33.0",
    "telebirr-receipt": "^0.0.1",
    "telegraf": "^4.15.0",
    "tesseract.js": "^7.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/pdfkit": "^0.13.2",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "fast-check": "^3.14.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^11.0.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
